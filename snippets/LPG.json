{
    "Let variable": {
        "prefix": "let",
        "body": [
            "let ${1:name} : ${2:type}"
        ],
        "description": "Creates a variable defintion"
    },
    "Enum": {
        "prefix": "enum",
        "body": [
            "let ${1:name} = enum",
            "\t${2:value}"
        ],
        "description": "Creates a variable defintion that defines an enum"
    },
    "Function": {
        "prefix": "function",
        "body": [
            "let ${1:name} = (${2:parameters}) => ",
            "\t${3:body}"
        ],
        "description": "Creates a function defintion"
    },
    "Creating an Interface": {
        "prefix": "interface",
        "body": [
            "let ${1:name} = interface",
            "\t${2:body}"
        ],
        "description": "Creates a basic interface defintion"
    },
    "Creating an implementation": {
        "prefix": "impl",
        "body": [
            "impl ${1:interface-type} for ${2:base-type}:",
            "\t${3:method implementation}"
        ],
        "description": "Creates a basic implmentation for an interface"
    },
    "Importing another module": {
        "prefix": "import",
        "body": [
            "let ${1:handle} = import ${1:handle}"
        ],
        "description": "Imports a module"
    }
}